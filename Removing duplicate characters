#include <bits/stdc++.h>
using namespace std;
void removeDuplicate(char str[]){
    if(str[0] == '\0')
        return;
    if(str[0] != str[1])
        removeDuplicate(str+1);
    else{
        int i=1;
        for(;str[i] != '\0';i++)
            str[i-1] = str[i];
        str[i-1]=str[i];
        removeDuplicate(str);
    }
    return;
}
int main(){
    char str[100];
    char ch,rep_ch;
    cin>> str;
    removeDuplicate(str);
    cout<<str<<endl;
    return 0;
}

output:-
Input --> aabaabbddbccbb
Answer --> ababdbcb
