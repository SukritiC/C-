#include <bits/stdc++.h>
using namespace std;
void removeChar(char str[],char ch){
    if(str[0] == '\0')
        return;
    if(str[0] != ch)
        removeChar(str+1,ch);
    else{
        int i=1;
        for(;str[i] != '\0';i++)
            str[i-1] = str[i];
        str[i-1]=str[i];
        removeChar(str,ch);
    }
    return;

}
int main(){
    char str[100];
    char ch;
    cin>> str;
    cout<<"Enter character to remove"<<endl;
    cin>>ch;
    removeChar(str,ch);
    cout<<str<<endl;;
    return 0;
}


