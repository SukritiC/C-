
#include<iostream>
using namespace std;
int sumOfArray(int arr[],int len){
    if(len == 0)
        return 0;
    return arr[0]+sumOfArray(arr+1,len-1);
}
int main(){
    int arr[10], length;
    cin>>length;
    for(int i=0;i<length;i++)
        cin>>arr[i];
    cout<<sumOfArray(arr,length)<<endl;
    return 0;
}
