
#include<iostream>
using namespace std;
bool elementSearch(int arr[],int len,int num){
    if (len == 0)
        return false;
    if (arr[0] == num)
        return true;
    return elementSearch(arr+1,len-1,num);
}
int main(){
    int arr[10], length, findNum;
    cin>>length;
    for(int i=0;i<length;i++)
        cin>>arr[i];
    cout <<"Enter number to be search"<<endl;
    cin>>findNum;
    cout<<elementSearch(arr,length,findNum)<<endl;
    return 0;
}

