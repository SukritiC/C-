#include <bits/stdc++.h>
using namespace std;


void elementSearch(int arr[],int len,int num, int index){
    if (len == 0)
        return;
    if (arr[index] == num)
        cout<<index<<" ";
    elementSearch(arr,len-1,num,index+1);
}
int main(){
    int arr[10], length, findNum;
    cin>>length;
    for(int i=0;i<length;i++)
        cin>>arr[i];
    cout <<"Enter number to be search"<<endl;
    cin>>findNum;
    elementSearch(arr,length,findNum,0);
    return 0;
}






